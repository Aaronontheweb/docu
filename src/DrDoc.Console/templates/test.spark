<table>
	<tr>
		<th>Namespace</th>
		<th>Types</th>
	</tr>
	<tr each="var ns in Namespaces">
		<td>${ns.Name}</td>
		<td>
      <table>
        <tr>
          <th>Type</th>
          <th>Methods</th>
          <th>Properties</th>
        </tr>
        <tr each="var type in ns.Types">
          <td>${type.Name}</td>
          <td>
            <ul>
              <li each="var method in type.Methods">${method.Name}</li>
            </ul>
          </td>
          <td>
            <ul>
              <li each="var property in type.Properties">${property.Name}</li>
            </ul>
          </td>
        </tr>
      </table>
    </td>
	</tr>
</table>